          <stringProp name="query">SELECT BA.BILLING_ACCOUNT_NUM FROM ODSCM.customer_individual cui, ODSCM.customer cust, ODSCM.billing_arrangement bir, 
ODSCM.pay_channel pc,ODSCM.billing_account ba LEFT OUTER JOIN ODSCM.cust_contact_indvdl_asgnmt ccia ON BA.CUSTOMER_ID = ccia.CUSTOMER_ID AND 
CCIA.CONTACT_ROLE_TYPE_CD = &apos;N&apos; AND ccia.EFF_END_DT IS NULL LEFT OUTER JOIN ODSCM.contact_individual ci ON 
ccia.CONTACT_INDVDL_ID = ci.CONTACT_INDVDL_ID LEFT OUTER JOIN ODSCM.contact_indvdl_elec_cntct ciec ON 
ccia.CONTACT_INDVDL_ID = ciec.CONTACT_INDVDL_ID 
AND ciec.EFF_END_DT IS NULL and ciec.ELEC_ADDR_SUBTYPE_CD =&apos;EPR&apos; WHERE BA.CUSTOMER_ID = cui.CUSTOMER_ID AND cui.EFF_END_DT is null and 
BA.CUSTOMER_ID = cust.CUSTOMER_ID and BA.BILLING_ACCOUNT_ID = bir.BILLING_ACCOUNT_ID and 
BIR.BILLING_ARRANGEMENT_ID = pc.BILLING_ARRANGEMENT_ID AND ba.coll_start_dt IS NULL and ba.BACCT_STATUS_CD =&apos;O&apos; and 
cust.CUST_TYPE_CD = &apos;R&apos; and rownum &lt; 500000</stringProp>
